<b>Total Count of Inventories : <%=@inventory_transfers.count%></b>
 <div class="col-md-12 section-top-15 wow fadeInDown" data-wow-duration="1s"
                             data-wow-delay="0.2s">
       <div class="panel panel-primary">
            <div class="panel-heading">Transfer Details
        </div>
 <div class="panel-body">
<p id="notice"></p>
 <div class="box-content" style="overflow-y: auto;height: 550px;width:100%">
<table class="table table-striped table-bordered responsive">
    <tr>
      <th><b>Branch</b></th>
      <th><b>Part no</b></th>
      <th><b>Description</b></th>
       <th><b>Total stock qty Transfered</b></th>
      <th><b>Warehouse stock qty</b></th>
      <th><b>Engineer stock qty</b></th>
      <th><b>Map</b></th>
      <th><b>Total stock value</b></th>
      <th><b>Status</b>/</th>
      <th><b>Latest modify date</b></th>
      <th><b>Remark</b></th>
      <th><b>Item Transfered Qty </b></th>
      <th><b>Item Transfered From </b></th>
       <th><b>Inventory type To </b></th>
    </tr>
  </thead>

  <tbody>

    <%@inventories.each do |inventory|%>
      <tr>
       <%if inventory.transfer_id.present?%>
        <td><%= inventory.branch %></td>
        <td><%= inventory.part_no %></td>
        <td><%= inventory.description %></td>
        <td><%= inventory.transfered_stock_qty %></td>
        <td><%= inventory.warehouse_stock_qty %></td>
        <td><%= inventory.engineer_stock_qty %></td>
        <td><%= inventory.map %></td>
        <td><%= inventory.total_stock_value %></td>
        <td><%= inventory.status %></td>
        <td><%= inventory.latest_modify_date %></td>
       
        <td><%= inventory.transfer.remark%></td>
         <td><%= inventory.transfer.transfer_item%></td>
        <td><%= inventory.transfer.inventory_type.name%></td>
        <td><%= inventory.inventory_type.name %></td>
      <%end%>
      </tr>
      <%end%>
  </tbody>
</table>
<br>
</div></div></div></div>