
<div class="col-md-12 section-top-15 wow fadeInDown" data-wow-duration="1s"
                             data-wow-delay="0.2s">
        <div class="panel panel-primary">
            <div class="panel-heading">Report
        </div>
      <%= form_for :transaction, url:{action: 'transactions_list'} do |f| %>
     
      <div class="row">
        <div class="col-sm-2"></div>
        <div class="col-sm-5">
          <div class="form-group">
            <b>Start Date:</b>
            <span style="color:red">*</span>
            <%= f.date_field :start_date,class: "form-control datepicker" %>
          </div>
          <div class="form-group">
             <b>End Date:</b>
             <span style="color:red">*</span>
            <%= f.date_field :end_date,class: "form-control datepicker" %>
          </div>
          <div class="form-group">
             <b>Status:</b>
             <span style="color:red">*</span>
            <%=f.select :status,["Pending","Work-Done","Good Delivered"],{prompt: 'Select Status'},class: "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-sm-offset-4">
          <%= f.submit 'View', class: "btn btn-success btn-lg"%>
      </div>
    </fieldset>        
  <% end %>
</div>
</div>
</div>
</div>