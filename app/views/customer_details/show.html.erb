<div class="col-md-12 section-top-15 wow fadeInDown" data-wow-duration="1s"
                             data-wow-delay="0.2s">
        <div class="panel panel-primary">
            <div class="panel-heading">Acknowledgment of Service Request
        </div>
                    <div class="panel-body">


<%= link_to 'Back', customer_details_path%><b> | </b>
<%= link_to 'Download Customer PDF ', customer_detail_path(@customer_detail, format: 'pdf'),target: '_blank' %> <b> | </b>

<%= link_to 'Download Engineer PDF', engineer_detail_path(@engineer_detail,format: 'pdf'),target: '_blank' %>


                        <div class="row form-group">
                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Customer Name:</b></label>&nbsp;
                                  
                                    <%= @customer_detail.customer_name %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Date:</b></label>&nbsp;
                                  
                                    <%= @customer_detail.date %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Address:</b></label>&nbsp;
                                    
                                    <%= @customer_detail.address %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Customer No:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.customer_no %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Telephone No:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.telephone_no %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Mobile No:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.mobile_no %>
