                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Purchase Date:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.purchase_date %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Serial No:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.serial_no %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Full warranty:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.full_warranty %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Labor only:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.labor_only %>
