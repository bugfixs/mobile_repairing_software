                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Defect Code:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.defect_code%>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Repair Code:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.repair_code%>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Status:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.status%>
                            </div>



  
</div>
</div>
</div>

