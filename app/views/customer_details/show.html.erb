                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Out of warranty:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.out_of_warranty%>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Repair Received:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.repair_received%>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Repair Completed:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.repair_completed %>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Good Delivered:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.return_by_date%>
                            </div>

                            <div class="col-md-4">
                                <label for="text_input" class="label_height"><b>Defect Description:</b></label>&nbsp;
                                   
                                    <%= @customer_detail.defect_description%>
