<!DOCTYPE html>
<html>
  <head>
    <title>QsetApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div id="user_nav">
  <% if user_signed_in? %>
    
    <%= link_to "Sign out", destroy_user_session_path,method: :delete %>
  <% else %>
  <%= render 'layouts/nav'%>
  <% end %>
</div>
    <%= yield %>
  </body>
</html>
