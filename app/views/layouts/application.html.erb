<!DOCTYPE html>
<html>
  <head>
    <title>QsetApp</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
     <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
 
  <% if user_signed_in? %>
      <%= render 'layouts/nav'%>
  <% else %>
  <%= render 'layouts/nav'%>
  <% end %>
  <%= yield %>
  </body>
</html>
